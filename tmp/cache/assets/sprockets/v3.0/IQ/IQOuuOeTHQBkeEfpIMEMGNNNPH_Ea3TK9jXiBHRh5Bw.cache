<div class="row-fluid">
    <div class="col-md-12">
        <h1>New Event</h1>
        <div ng-if="flash" class="alert alert-success" role="alert">{{flash}}</div>
        <form ng-submit="addEvent()"
              style="margin-top:30px;">
            <div class="form-group">

            </div>


            <div class="form-group">

                <input class="form-control" date-time ng-model="dates.today"
                                           id="pickerMinSelector"
                                           date-change="changeMinMax"
                                           max-date="maxDate" view="date"
                                           timezone="UTC"
                                           format="lll"
                >
            </div>
            <div class="form-group">
        <textarea type="text"
                  class="form-control"
                  placeholder="Description"
                  ng-model="description"></textarea>
            </div>

            <button type="submit" class="btn btn-primary">–î–æ–±–∞–≤–∏—Ç—å</button>

        </form>
    </div>
</div>
<div class="clearfix"></div>
<br><br>
<div class="row-fluid">
    <div class="col-md-4">
        <div class="form-group">
         <input type="text"
               class="form-control"
               placeholder="Name"
               ng-model="guest.name"></input>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <input type="text"
               class="form-control"
               placeholder="Surname"
               ng-model="guest.surname"></input>
        </div>
    </div>
    <div class="col-md-2">
        <button ng-click="addGuest()">+</button>
    </div>
</div>

<div class="row-fluid">
    <div class="col-md-12">
        <h4>–î–æ–±–∞–≤–ª–µ–Ω–Ω—ã–µ –≥–æ—Å—Ç–∏:</h4>
        <ul ng-repeat="guest in guests track by $index">
            <li>{{guest.name}} {{guest.surname}}</li>
        </ul>
    </div>
</div>
<div class="clearfix"></div>
<div class="form-group">
    <h4>Upload on file select</h4>
    <button type="file" ngf-select="upload($files, $invalidFiles)"
            accept="image/*" ngf-max-height="1000" ngf-max-size="1MB">
        Select File</button>
    <br><br>
    File:
    <div style="font:smaller">{{f.name}} {{errFile.name}} {{errFile.$error}} {{errFile.$errorParam}}
        <span class="progress" ng-show="f.progress >= 0">
          <div style="width:{{f.progress}}%"
               ng-bind="f.progress + '%'"></div>
      </span>
    </div>
    {{errorMsg}}
</div>
